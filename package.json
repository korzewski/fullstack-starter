    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.35",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "prettier": "2.4.1",
    "prisma": "^3.5.0",
    "tailwindcss": "^2.2.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "prisma": {
